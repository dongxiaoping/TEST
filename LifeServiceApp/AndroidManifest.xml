<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.lifeserviceapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.project.lifeserviceapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.project.lifeserviceapp.publicservice.shopservice.ShopNameListActivity"
            android:label="@string/title_activity_shop_name_list" >
        </activity>
        <activity
            android:name="com.project.lifeserviceapp.publicservice.shopservice.GoodsListActivity"
            android:label="@string/title_activity_goods_list" >
        </activity>
        <activity
            android:name="com.project.lifeserviceapp.publicservice.shopservice.SubmitOrdersActivity"
            android:label="@string/title_activity_submit_orders" >
        </activity>
    </application>

</manifest>
